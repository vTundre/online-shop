DROP TABLE public."user";

CREATE TABLE public."user"
(
    id integer,
    name varchar(100),
    password char(100),
    salt char(100),
    role char(100)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public."user"
    OWNER to postgres;


CREATE TABLE product(id integer,  name char(100), description char(100), price integer)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.product
    OWNER to postgres;

